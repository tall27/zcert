# zcert Profile Configuration File
# This file supports multiple profiles with different ZTPKI settings
# Use: zcert -config zcert.cnf --cn "mycert.com" 
#   or zcert -config zcert.cnf -profile p12 --cn "mycert.com"

[Default]
# Default profile used when no -profile is specified
url = https://ztpki-dev.venafi.com/api/v2
key-id = 165c01284c6c8d872091aed0c7cc0149
secret = b431afc1ed6a6b7db5f760671840efa14224be60a11e0c164a6d0d021a45748c
algo = sha256
format = pem
policy = WebServer
key-size = 2048
key-type = rsa
chain = true

[p12]
# Profile for PKCS#12 certificates
url = https://ztpki-dev.venafi.com/api/v2
key-id = your-p12-key-id
secret = your-p12-secret
algo = sha256
format = p12
policy = WebServer
p12-password = secretpass123
key-size = 2048
key-type = rsa
chain = true

[test]
# Profile for testing environment
url = https://ztpki-dev.venafi.com/api/v2
key-id = test-key-id
secret = test-secret
algo = sha256
format = pem
policy = TestPolicy
key-size = 2048
key-type = rsa
chain = false

[prod]
# Profile for production environment
url = https://ztpki-dev.venafi.com/api/v2
key-id = prod-key-id
secret = prod-secret
algo = sha256
format = pem
policy = ProductionWebServer
key-size = 4096
key-type = rsa
chain = true
no-key-output = false
